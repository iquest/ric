<%= rug_form_for(@user, url: account_password_path) do |f| %>
	<%= f.text_input_row :current_password, :password_field %>
	<%= f.text_input_row :password, :password_field %>
	<%= f.text_input_row :password_confirmation, :password_field %>
	<%= f.primary_button_row :submit, label: t("helpers.submit.update") %>
<% end %>
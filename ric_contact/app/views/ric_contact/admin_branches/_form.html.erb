<%= rug_form_for(@branch, url: (@branch.id.nil? ? branches_path : branch_path(@branch))) do |f| %>
	<%= f.text_input_row :name, :text_field %>
	<%= f.address_row :address %>
	<%= f.text_input_row :url, :text_field %>
	<%= f.text_input_row :latitude, :text_field %>
	<%= f.text_input_row :longitude, :text_field %>
	<%= f.primary_button_row :submit %>
<% end %>

<%= rug_form_for(@gallery_image, create_url: :images_path, update_url: :image_path) do |f| %>
	<%= f.hidden_field :gallery_directory_id %>
	<%= f.dropzone_row :image, append_columns: { gallery_directory_id: true }  %>
	<%= f.text_input_row :title, :text_field %>
	<%= f.primary_button_row :submit %>
<% end %>

<%= rug_form_for(@notification, create_url: :notifications_path, update_url: :notification_path) do |f| %>
	<%= f.text_input_row :subject %>
	<%= f.text_area_row :message %>
	<%= f.picker_row :kind %>
	<%= f.text_input_row :url %>
	<%= f.primary_button_row :submit %>
	<%= f.back_link_button_row %>
<% end %>

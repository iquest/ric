<%= rug_index(@partners) do |t| %>
	<%= t.body do |b| %>
		<%= b.action :move, path: :move_partner_path %>
		<%= b.string :name, show: { path: :partner_path } %>
		<%= b.string :url %>
		<%= b.action :edit, path: :edit_partner_path %>
		<%= b.action :destroy, path: :partner_path %>
	<% end %>
	<%= t.footer do |f| %>
		<%= f.pagination %>
		<%= f.summary %>
	<% end %>
<% end %>

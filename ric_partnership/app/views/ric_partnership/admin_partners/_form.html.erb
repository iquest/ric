<%= rug_form_for(@partner, create_url: :partners_path, update_url: :partner_path) do |f| %>
	<%= f.text_input_row :name, :text_field, localization: (RicPartnership.localization == true) %>
	<%= f.text_input_row :url %>
	<%= f.dropzone_row :logo %>
	<%= f.primary_button_row :submit %>
	<%= f.back_link_button_row %>
<% end %>

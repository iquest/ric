<%= rug_form_for(@user, url: ric_auth_admin.session_path) do |f| %>
	<%= f.text_input_row :email, :email_field %>
	<%= f.text_input_row :password, :password_field %>
	<% if RicAuth.user_model.rememberable? %>
		<%= f.checkbox_row :remember_me %>
	<% end %>
	<%= f.primary_button_row :submit, label: t("headers.ric_auth.admin_sessions.new") %>
<% end %>
<%= rug_index(@slugs) do |t| %>
	<%= t.body do |b| %>
		<%= b.string :locale %>
		<%= b.string :original %>
		<%= b.string :translation %>
		<%= b.string :filter if RicUrl.use_filter == true %>
		<%= b.action :edit, path: :edit_slug_path %>
		<%= b.action :destroy, path: :slug_path %>
	<% end %>
	<%= t.footer do |f| %>
		<%= f.pagination %>
		<%= f.summary %>
	<% end %>
<% end %>
